#bloquea directorios del proyecto
Options ALL -Indexes
#para poder utilizar rewriterule
RewriteEngine on
#convierte  las palabras en variables get
RewriteRule ^(\w+)$ index.php?ubicacion=$1
RewriteRule ^Validacion=(\w+)$ index.php?ubicacion=Validar&key=$1
RewriteRule ^RegistrarProyecto=(\w+)$ index.php?ubicacion=RegistrarProyecto&id=$1
RewriteRule ^Proyecto=(\w+)$ index.php?ubicacion=Proyecto&id=$1
RewriteRule ^Feria=(\w+)$ index.php?ubicacion=Feria&id=$1